  "module": "dist/lib/esm/index.js",
  "types": "dist/lib/esm/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Scott Bamford",
    "email": "scott@ambidect.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottbamford/reactstrap-elasticinput.git"
  },
  "keywords": [
    "typescript",
    "react",
    "reactstrap",
    "input",
    "textarea",
    "elastic",
    "autosize",
    "resize"
  ],
  "publishConfig": {
